  "compilerOptions": {
    "lib": ["deno.ns","dom"],
    "checkJs": true
  },
  "tasks": {
    "dev": "deno run -A server.js",
    "migrate": "deno run -A ./node_modules/.bin/node-pg-migrate"
  },
  "imports": {
    "@oak/oak": "jsr:@oak/oak@^17.1.0",
    "@std/assert": "jsr:@std/assert@1",
    "@types/express": "npm:@types/express@^5.0.0",
    "connect-livereload": "npm:connect-livereload@^0.6.1",
    "dotenv": "npm:dotenv@^16.4.5",
    "ejs": "npm:ejs@^3.1.10",
    "express": "npm:express@^5.0.1",
    "express-jwt": "npm:express-jwt@^8.4.1",
    "livereload": "npm:livereload@^0.9.3",
    "node-pg-migrate": "npm:node-pg-migrate@^7.7.1",
    "pg": "npm:pg@^8.13.1",
    "pg-promise": "npm:pg-promise@^11.10.1"
  }
}
